{
  "manifest_version": 2,
  "name": "Giphy Search",
  "description": "Adds a search engine for Giphy to quickly find the right gif for the right occasion",
  "version": "1.0",
  "applications": {
    "gecko": {
      "strict_min_version": "55.0"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Giphy",
      "search_url": "https://www.giphy.com/search/?q={searchTerms}",
      "keyword": "gif",
      "favicon_url": "https://www.giphy.com/favicon.ico",
      "is_default": false
    }
  }
}
